    "C_Cpp.updateChannel": "Insiders",
    "editor.cursorStyle": "line",
    "editor.insertSpaces": false,
    "editor.lineNumbers": "on",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "window.zoomLevel": 1,
    "cmake.configureOnOpen": true,
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "julia.enableTelemetry": true,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "vim.autoSwitchInputMethod.obtainIMCmd": "C:\\im-select.exe",
    "vim.autoSwitchInputMethod.switchIMCmd": "C:\\im-select.exe {im}",
    "vim.autoSwitchInputMethod.enable": true,
    "vim.autoSwitchInputMethod.defaultIM": "1033",
    "clangd.path": "c:\\Users\\YWH\\AppData\\Roaming\\Code\\User\\globalStorage\\llvm-vs-code-extensions.vscode-clangd\\install\\15.0.3\\clangd_15.0.3\\bin\\clangd.exe",
    "http.proxySupport": "off",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "vim.foldfix": true,
    "vim.leader": " ",
    "vim.easymotion": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "H"
            ],
            "commands": [
                "workbench.action.navigateBack"
            ]
        },
        {
            "before": [
                "L"
            ],
            "commands": [
                "workbench.action.navigateForward"
            ]
        },
        {
            "before": [
                "J",
                "p"
            ],
            "commands": [
                "latex-workshop.synctex"
            ]
        }
    ],
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "h"
            ],
            "commands": [
                "editor.action.triggerSuggest"
            ],
            "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
        }
    ],
    "julia.symbolCacheDownload": true,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]",
        "REVIEW",
        "OLD"
    ],
    "window.enableMenuBarMnemonics": false,
    "latex-workshop.latex.pdfWatch.delay": 5000,
    "notebook.lineNumbers": "on",
    "editor.cursorSurroundingLines": 5,
    "doxdocgen.generic.paramTemplate": "\\param {param} ",
    "doxdocgen.generic.returnTemplate": "\\return {type} ",
    "doxdocgen.generic.dateTemplate": "\\date {date}",
    "doxdocgen.generic.briefTemplate": "\\brief {text}",
    "doxdocgen.generic.authorTag": "\\author {author} ({email})",
    "doxdocgen.file.versionTag": "\\version 0.1",
    "doxdocgen.file.fileTemplate": "\\file {name}",
    "doxdocgen.cpp.tparamTemplate": "\\tparam {param} ",
    "window.commandCenter": false,
    "git.ignoreRebaseWarning": true,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "editor.inlayHints.enabled": "on",
    "clangd.arguments": [
        "-header-insertion=never"
    ]
}